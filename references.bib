}


@article{Roberts2017,
 abstract = {In this article, I seek to update the sociogenomic model of personality traits (Roberts {\&} Jackson, 2008). Specifically, I seek to outline a broader and more comprehensive theoretical perspective on personality traits than offered in the original version of the sociogenomic model of personality traits. First, I review the major points of our 2008 article. Second, I update our earlier model mostly with insights derived from a deeper reading of evolutionary theoretical systems, such as those found in life-history theory and ecological-evolutionary-developmental biology. In particular, this revision incorporates two evolutionary-informed systems, labeled pliable and elastic systems, that provide new insights into how personality traits develop. Third, I describe some of the implications of this new understanding of the biological and evolutionary architecture that underlies human phenotypes such as personality traits.},
 author = {Roberts, Brent W.},
 year = {2017},
 title = {A revised sociogenomic model of personality traits},
 issn = {0022-3506},
 journal = {Journal of Personality},
 doi = {10.1111/jopy.12323},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/28509389}
}


