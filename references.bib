}


@article{Malanchini2017,
 abstract = {Extant literature has established a consistent association between aspects of reading motivation, such as enjoyment and self-perceived ability, and reading achievement, in that more motivated readers are generally more skilled readers. However, the developmental etiology of this relation is yet to be investigated. The present study explores the development of the motivation-achievement association and its genetic and environmental underpinnings. Applying cross-lagged design in a sample of 13,825 twins, we examined the relative contribution of genetic and environmental factors to the association between reading enjoyment and self-perceived ability and reading achievement. Children completed a reading comprehension task and self-reported their reading enjoyment and perceived ability twice in middle childhood: when they were 9-10 and 12 years old. Results showed a modest reciprocal association over time between reading motivation (enjoyment and perceived ability) and reading achievement. Reading motivation at age 9-10 statistically predicted the development of later achievement, and similarly, reading achievement at age 9-10 predicted the development of later motivation. This reciprocal association was observed beyond the stability of the variables and their contemporaneous correlation and was largely explained by genetic factors. (PsycINFO Database Record},
 author = {Malanchini, Margherita and Wang, Zhe and Voronin, Ivan and Schenker, Victoria J. and Plomin, Robert and Petrill, Stephen A. and Kovas, Yulia},
 year = {2017},
 title = {Reading self-perceived ability, enjoyment and achievement: A genetically informative study of their reciprocal links over time},
 keywords = {Achievement;Child;Comprehension;Female;Gene-Environment Interaction;Happiness;Humans;Language Tests;Longitudinal Studies;Male;Multivariate Analysis;Reading;Self Concept;Self Report;Time Factors;Twins, Dizygotic;Twins, Monozygotic},
 pages = {698--712},
 volume = {53},
 number = {4},
 issn = {1939-0599},
 journal = {Developmental Psychology},
 doi = {10.1037/dev0000209},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/28333527},
 file = {https://doi.org/10.1037/dev0000209}
}


@article{Maldonado1993,
 abstract = {In the absence of prior knowledge about population relations, investigators frequently employ a strategy that uses the data to help them decide whether to adjust for a variable. The authors compared the performance of several such strategies for fitting multiplicative Poisson regression models to cohort data: 1) the {\textquotedbl}change-in-estimate{\textquotedbl} strategy, in which a variable is controlled if the adjusted and unadjusted estimates differ by some important amount; 2) the {\textquotedbl}significance-test-of-the-covariate{\textquotedbl} strategy, in which a variable is controlled if its coefficient is significantly different from zero at some predetermined significance level; 3) the {\textquotedbl}significance-test-of-the-difference{\textquotedbl} strategy, which tests the difference between the adjusted and unadjusted exposure coefficients; 4) the {\textquotedbl}equivalence-test-of-the-difference{\textquotedbl} strategy, which significance-tests the equivalence of the adjusted and unadjusted exposure coefficients; and 5) a hybrid strategy that takes a weighted average of adjusted and unadjusted estimates. Data were generated from 8,100 population structures at each of several sample sizes. The performance of the different strategies was evaluated by computing bias, mean squared error, and coverage rates of confidence intervals. At least one variation of each strategy that was examined performed acceptably. The change-in-estimate and equivalence-test-of-the-difference strategies performed best when the cut-point for deciding whether crude and adjusted estimates differed by an important amount was set to a low value (10{\%}). The significance test strategies performed best when the alpha level was set to much higher than conventional levels (0.20).},
 author = {Maldonado, G. and Greenland, S.},
 year = {1993},
 title = {Simulation study of confounder-selection strategies},
 pages = {923--936},
 volume = {138},
 number = {11},
 journal = {American journal of epidemiology},
 doi = {10.1093/oxfordjournals.aje.a116813},
 file = {http://www.ncbi.nlm.nih.gov/pubmed/8256780}
}


