 keywords = {Achievement;Adolescent;Child;Curriculum;Educational Measurement;Emigrants and Immigrants/education/psychology;Female;Germany;Humans;Internal-External Control;Longitudinal Studies;Male;Mathematics;Models, Educational;Self Concept;Verbal Learning;Vulnerable Populations},
 pages = {591--611},
